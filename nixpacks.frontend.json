{
  "phases": {
    "setup": {
      "nixPkgs": ["nodejs-18_x"]
    },
    "install": {
      "cmds": [
        "cd ihm-frontend",
        "npm install"
      ]
    },
    "build": {
      "cmds": [
        "cd ihm-frontend",
        "npm run build"
      ]
    }
  },
  "start": {
    "cmd": "cd ihm-frontend && npm run preview -- --host 0.0.0.0 --port $PORT"
  }
}
